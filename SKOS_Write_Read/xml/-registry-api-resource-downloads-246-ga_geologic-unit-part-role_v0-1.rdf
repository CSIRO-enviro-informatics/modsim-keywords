	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/stratigraphic_part"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/tectonic_block"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/unspecified_part_role"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/vein_or_dike_lithosome"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunitpartrole/xenolith"/>
</rdf:Description>

