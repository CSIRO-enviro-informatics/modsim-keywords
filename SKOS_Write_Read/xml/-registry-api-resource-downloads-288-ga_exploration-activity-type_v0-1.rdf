	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/mining-pilot"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/ore-beneficiation-tests"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/ore-deposit-evaluation"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/percussion-drilling"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/percussion-drilling-assessment"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/exploration-activity-type/regional-geochemistry"/>
