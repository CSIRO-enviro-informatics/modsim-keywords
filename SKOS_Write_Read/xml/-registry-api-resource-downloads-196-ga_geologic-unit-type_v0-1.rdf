	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunittype/magnetostratigraphic_unit"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunittype/mass_movement_unit"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunittype/pedoderm"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunittype/pedostratigraphic_unit"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/geologicunittype/polarity_chronostratigraphic_unit"/>
</rdf:Description>

<rdf:Description rdf:about="http://purl.org/dc/terms/title">
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Resource"/>
