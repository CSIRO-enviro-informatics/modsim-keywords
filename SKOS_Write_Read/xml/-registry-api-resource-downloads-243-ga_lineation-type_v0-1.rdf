	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/slickenline"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/stretching_lineation"/>
	<skos:member rdf:resource="http://resource.geosciml.org/classifier/cgi/lineationtype/tectonic_lineation"/>
</rdf:Description>

